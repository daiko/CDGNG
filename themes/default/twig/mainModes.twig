<form action="index.php?view=csvModes" method="post">
  <button id="exportTableauModalite" class="btn btn-info" type="submit">
    <i class="icon-download-alt icon-white"></i>
    Exporter
  </button>
</form>
<table class="table table-striped table-bordered table-condensed">
  <thead>
    <tr>
      <th>Code</th>
      <th>Intitulé</th>
      <th>Description</th>
    </tr>
  </thead>

  <tbody>
    {% for code, mode in modes %}
      <tr>
        <td>{{ code }}</td>
        <td>{{ mode.Intitulé }}</td>
        <td>{{ mode.Description }}</td>
      </tr>
    {% endfor %}
  </tbody>
</table>
